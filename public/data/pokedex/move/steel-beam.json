    "flavor_text_entries": [
        {
            "flavor_text": "\u305c\u3093\u3057\u3093\u304b\u3089\u3000\u3042\u3064\u3081\u305f\u3000\u306f\u304c\u306d\u3092\n\u30d3\u30fc\u30e0\u3068\u3057\u3066\u3000\u306f\u3052\u3057\u304f\u3000\u3046\u3061\u3060\u3059\u3002\n\u3058\u3076\u3093\u3082\u3000\u30c0\u30e1\u30fc\u30b8\u3092\u3000\u3046\u3051\u3066\u3057\u307e\u3046\u3002",
