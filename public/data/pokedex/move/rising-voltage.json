    "flavor_text_entries": [
        {
            "flavor_text": "\u3058\u3081\u3093\u304b\u3089\u3000\u305f\u3061\u306e\u307c\u308b\u3000\u3067\u3093\u3052\u304d\u3067\u3000\u3053\u3046\u3052\u304d\u3002\n\u3042\u3044\u3066\u304c\u3000\u30a8\u30ec\u30ad\u30d5\u30a3\u30fc\u30eb\u30c9\u306b\u3000\u3044\u308b\u3068\u304d\n\u308f\u3056\u306e\u3000\u3044\u308a\u3087\u304f\u304c\u3000\uff12\u3070\u3044\u306b\u3000\u306a\u308b\u3002",
